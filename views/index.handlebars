   <script>
        let textBox = document.querySelector('#quienes-somos .main-text');
        gsap.from('#quienes-somos .main-text',{
            scrollTrigger:"#quienes-somos",
            x:1000,
            duration:2
        })
        gsap.from('#quienes-somos .header-container h2',{
            scrollTrigger:"#quienes-somos",
            x:1000,
            duration:2
        })
        gsap.from('#quienes-somos .img-container img',{
            scrollTrigger:"#quienes-somos",
            x:-1000,
            duration:2
        })
   </script>
</body>
</html>